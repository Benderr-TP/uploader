{
  "manifest_version": 2,
  "name": "Tidepool Device Uploader",
  "short_name": "Uploader",
  "version": "0.1",
  "minimum_chrome_version": "32",
  "icons": {
    "16": "T_icon_dark_16.png",
    "32": "T_icon_dark_32.png",
    "64": "T_icon_dark_64.png",
    "128": "T_icon_dark_128.png",
    "512": "T_icon_dark_512.png"
  },
  "permissions": [
    "http://localhost/",
    "https://*.tidepool.io/",
    "fileSystem",
    "system.storage",
    "serial",
    "usb",
  {
    "usbDevices": [
      {
        "deviceName": "Dexcom G4 CGM",
        "vendorId": 8867,
        "productId": 71
      },
      {
        "deviceName": "Asante SNAP",
        "vendorId": 1027, 
        "productId": 32568
      },
      {
        "deviceName": "Kindle PaperWhite",
        "vendorId": 6473, 
        "productId": 4
      },
      {
        "deviceName": "Garmin 500",
        "vendorId": 2334, 
        "productId": 9228
      },
      {
        "deviceName": "Arduino Esplora",
        "vendorId": 9025, 
        "productId": 32828
      }
    ]
  }
  ],
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  }
}
