{
  "manifest_version": 2,
  "name": "Tidepool Device Uploader",
  "short_name": "Uploader",
  "version": "0.1",
  "minimum_chrome_version": "32",
  "icons": {
    "16": "T_icon_dark_16.png",
    "32": "T_icon_dark_32.png",
    "64": "T_icon_dark_64.png",
    "128": "T_icon_dark_128.png",
    "512": "T_icon_dark_512.png"
  },
  "permissions": [
    "fileSystem",
    "system.storage",
    "usb",
  {
    "usbDevices": [
      {
        "vendorId": 2334, 
        "productId": 9228
      }
    ]
  }
  ],
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  }
}
